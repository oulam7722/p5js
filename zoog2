<html>

<head>
    <title>p5js Template</title>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.5.3/p5.js"></script>
    
    <script>
        function setup() {
            createCanvas(600,800);
        }

       function draw() {
        fill(0,100,250) //the blue color for the text
        textSize(15) //this is the words"imdone"
        text("im done",80,240)
        fill(250,170,160) //the color of the face
        ellipse(100,100,100,100)//this is the head
        fill(0,250,250) //the color for the eyes
        ellipse(80,80,20,30) //this is the eyes
        ellipse(120,80,20,30)
        fill(250,0,0) //this is the red color for the nose
        ellipse(100,100,11,15) //this is the nose
        fill(0,0,0) //this gives the black color
        ellipse(78,90,11,15) //this is the black circle in the eyes
        ellipse(115,90,11,15)
        line(115,125,80,120) //this is the mouth
        line(90,150,70,250) //the next 3 lines are the dress
        line(110,150,140,250)
        line(140,250,70,250)
        line(120,250,120,300) //the next 2 lines are the feet
        line(90,250,90,300)
        line(110,150,180,190) //the next 2 lines are the hands 
        line(90,150,20,190)
        }
    </script>
</head>

<body>
<div id="code" class="container-fluid">
        <script src="https://gist-it.appspot.com/github/USERNAME/p5js/blob/gh-pages/FILENAME.html?footer=0"></script>
    </div>

    <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.12.0/jquery.min.js"></script>
    <script>
        $(window).bind("load", function() {
            $('#code').before($('#defaultCanvas0').remove());
        });
    </script>

</body>

